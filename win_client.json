{
  "log": {
    "disabled": false,
    "level": "warn",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "tag": "dns_proxy",
        "address": "https://dns.google/dns-query",
        "address_resolver": "dns_local",
        "detour": "select"
      },
      {
        "tag": "dns_direct",
        "address": "https://dns.alidns.com/dns-query",
        "address_resolver": "dns_local",
        "detour": "direct"
      },
      {
        "tag": "dns_block",
        "address": "rcode://success"
